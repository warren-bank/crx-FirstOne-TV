{
  "name": "FirstOne TV",
  "version": "0.2.1",
  "description": "Removes clutter and reduces CPU load.",
  "content_scripts": [{
    "matches": ["*://www.firstonetv.net/Live*"],
    "all_frames": true,
    "run_at": "document_start",
    "js": ["content.js"]
  }],
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "55": "img/icon55.png"
  },
  "manifest_version": 2
}
